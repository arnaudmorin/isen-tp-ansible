[general]
context=public
allowoverlap=no
udpbindaddr=0.0.0.0
tcpenable=no
tcpbindaddr=0.0.0.0
transport=udp,ws
srvlookup=no
realm={{ ansible_default_ipv4.address }}
externip={{ ansible_default_ipv4.address }}
;localnet=10.0.0.0/255.255.255.0

